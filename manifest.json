{
  "manifest_version": 2,
  "name": "Hello Chrome",
  "version": "0.1",
  "content_scripts": [
      {
        "matches": [
          "https://mail.google.com/*"
        ],
        "js": ["jquery-3.3.1.min.js", "content.js"]
      }
  ],
  "browser_action": {
      "default_icon": "icon.png"
  },
  "background":{
      "scripts": ["background.js"]
  }
}
